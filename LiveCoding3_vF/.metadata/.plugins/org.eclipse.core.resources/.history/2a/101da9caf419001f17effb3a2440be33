package com.practice;

import java.util.Scanner;

class MangosNaranjas {
	int mangos;
	int naranjas;
	
	public void cajas() {
		int caja;
		int cajaMangos = mangos / caja;
		int cajaNaranjas = naranjas / caja;
	}
}

public class Mangos_Y_Naranjas {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Ingresa el número de mangos: ");
		int mangos = sc.nextInt();
		System.out.println("Ingresa el número de naranjas: ");
		int naranjas = sc.nextInt();
		
		
    sc.close();
	}

}
